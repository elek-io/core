{
  "name": "@elek-io/core",
  "version": "0.5.0",
  "description": "Handles core functionality of elek.io Projects like file IO and version control.",
  "homepage": "https://elek.io",
  "repository": "https://github.com/elek-io/core",
  "bugs": {
    "url": "https://github.com/elek-io/core/issues"
  },
  "type": "module",
  "files": [
    "dist/node",
    "dist/browser"
  ],
  "main": "./dist/node/index.node.cjs",
  "module": "./dist/browser/index.browser.js",
  "types": "./dist/node/index.node.d.ts",
  "exports": {
    "node": {
      "types": "./dist/node/index.node.d.ts",
      "import": "./dist/node/index.node.js",
      "require": "./dist/node/index.node.cjs"
    },
    "browser": {
      "types": "./dist/browser/index.browser.d.ts",
      "import": "./dist/browser/index.browser.js",
      "require": "./dist/browser/index.browser.cjs"
    }
  },
  "scripts": {
    "lint": "tsc",
    "dev": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsup",
    "release": "changeset publish"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "file-type": "^19.0.0",
    "fs-extra": "^11.2.0",
    "is-svg": "^5.0.0",
    "lodash-es": "^4.17.21",
    "p-queue": "^8.0.1",
    "semver": "^7.5.4",
    "slugify": "^1.6.6",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@faker-js/faker": "^8.4.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18",
    "@types/semver": "^7.5.6",
    "@vitest/coverage-v8": "^1.2.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "dugite": "^2.7.1"
  }
}
