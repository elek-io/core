{
  "name": "@elek-io/core",
  "version": "0.15.1",
  "description": "Handles core functionality of elek.io Projects like file IO and version control.",
  "homepage": "https://elek.io",
  "repository": "https://github.com/elek-io/core",
  "bugs": {
    "url": "https://github.com/elek-io/core/issues"
  },
  "type": "module",
  "files": [
    "dist/node",
    "dist/browser"
  ],
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node/index.node.d.ts",
          "default": "./dist/node/index.node.js"
        }
      },
      "import": {
        "types": "./dist/browser/index.browser.d.ts",
        "default": "./dist/browser/index.browser.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint",
    "check-types": "tsc --noEmit",
    "check-format": "prettier --check . || exit 0",
    "format": "prettier --write .",
    "dev": "vitest",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsup",
    "release": "changeset publish"
  },
  "dependencies": {
    "@hono/node-server": "1.19.5",
    "@hono/swagger-ui": "0.5.2",
    "@hono/zod-openapi": "1.1.3",
    "@sindresorhus/slugify": "3.0.0",
    "fs-extra": "11.3.2",
    "hono": "4.10.3",
    "mime": "4.1.0",
    "p-queue": "8.1.1",
    "semver": "7.7.2",
    "uuid": "13.0.0",
    "winston": "3.18.2",
    "winston-daily-rotate-file": "5.0.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@eslint/js": "9.36.0",
    "@faker-js/faker": "10.0.0",
    "@tsconfig/node22": "22.0.2",
    "@tsconfig/strictest": "2.0.6",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.13.14",
    "@types/semver": "7.7.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.4.0",
    "jiti": "2.6.0",
    "prettier": "3.6.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.45.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "dugite": "2.7.1"
  }
}
